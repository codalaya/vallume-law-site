{
    "name": "vallume/main",
    "type": "project",
    "version": "1.0.0",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1.0",
        "ajcastro/eager-load-pivot-relations": "^0.3.0",
        "anandsiddharth/laravel-paytm-wallet": "^2.0",
        "awobaz/compoships": "^2.1",
        "bavix/laravel-wallet": "^9.6",
        "botman/botman": "^2.8",
        "botman/driver-web": "^1.5",
        "filament/filament": "^3.3",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^1.3",
        "laravel/cashier": "^14.8",
        "laravel/framework": "^10.0",
        "laravel/passport": "^11.0",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.7",
        "lawadvisor/laravel-installer": "dev-main",
        "maatwebsite/excel": "^3.1",
        "midtrans/midtrans-php": "^2.5",
        "minuteoflaravel/laravel-audio-video-validator": "^1.1",
        "mollie/laravel-mollie": "^2.14",
        "orangehill/iseed": "^3.0",
        "pusher/pusher-php-server": "^7.2",
        "pusher/pusher-push-notifications": "^2.0",
        "shift4/shift4-php": "^3.3",
        "spatie/laravel-translatable": "^6.3",
        "tightenco/ziggy": "^1.5"
    },
    "require-dev": {
        "authorizenet/authorizenet": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "itsgoingd/clockwork": "^5.1",
        "kitloong/laravel-migrations-generator": "^6.11",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./lib/*"
        }
    ],
    "autoload": {
        "files": [
            "app/Utils/Helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "platform-check": false
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
